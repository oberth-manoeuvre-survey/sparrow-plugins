{

  "refUpdates": [

    {
      name:  "refs/heads/%branch%",
      oldObjectId: "%old_object_id%"
    }

  ],
  
  commits: [
    {
      comment: "Initial commit.",
      changes: [
        {
          changeType: "add",
          item: {
            path: "%path%"
          },
          newContent: {
            content: "%content%",
            contentType: "rawtext"
          }
        }
      ]
    }
  ]
}

